	school = {Monash University},
	st = {A framework and pragmatic strategies for transit priority implementation},
	title = {A framework and pragmatic strategies for transit priority implementation},
	url = {https://bridges.monash.edu/articles/thesis/A_framework_and_pragmatic_strategies_for_transit_priority_implementation/13377680},
	volume = {PhD},
	year = {2020},
