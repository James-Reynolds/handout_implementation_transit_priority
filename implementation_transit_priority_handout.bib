@phdthesis{Reynolds:2020aa,
	abstract = {Bus lanes, traffic signals for trams, and many other measures can be used to prioritise on-road public transport services. Because buses and trams can carry people very efficiently there is often a strong case for advantaging them over other traffic, particular in congested urban areas. However, converting traffic lanes, removing on-street parking and other changes can mean that transit priority measures that are appropriate from an engineering perspective are often politically, institutionally or publicly unpopular. This research examines public transport priority through the lenses of legitimacy theory and public policy analysis, and develops pragmatic strategies for implementation in car-centric cities.},
	address = {Melbourne, VIC, Australia},
	author = {Reynolds, James},
	date-added = {2023-10-05 09:43:44 +1100},
